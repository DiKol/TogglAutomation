<Window x:Class="TogglAutomationApp.TrackWindow.TrackWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:CollectionViewMVVM.Views"
        xmlns:local="clr-namespace:TogglAutomationApp.TrackWindow" 
        
        mc:Ignorable="d"
                Title="TimerWindow" Height="170" Width="332" WindowStyle="None" ResizeMode="NoResize" AllowsTransparency="True" Background="White">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>


        <views:EmployeeListingView Grid.Row="1" Margin="0 10 0 0"
            DataContext="{Binding EmployeeListingViewModel}"
            FontSize="14" />
    </Grid>
</Window>

